public class countdigit 
{
    static int countdigits(int n)
    {
        int x=n; int count=0;
        while(x!=0)
        {
            x=x/10;
            count++;
        }
        return count;

    }
    public static void main(String args[])
    {
        int n=122345;
        System.out.println("Number is    "+n +"   and is  count is  "+countdigits(n));
    }
}
